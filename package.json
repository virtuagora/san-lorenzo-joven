    "vue-avatar": "^2.1.6",
    "vue-carousel": "^0.6.14",
    "vue-chartjs": "^3.4.0",
    "vue-infinite-loading": "^2.3.1",
    "vue-masonry": "^0.11.1",
    "vue-recaptcha": "^1.1.0",
    "vue-router": "^3.0.1",
    "vue-slider-component": "^2.7.5",
