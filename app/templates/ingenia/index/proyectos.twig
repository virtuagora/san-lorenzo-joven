  <div class="container is-fluid">
    <section class="section">
      <catalogo get-projects-url="{{path_for('lisPro')}}">
        <div class="notification has-text-centered">
          <i class="fas fa-cog fa-spin fa-lg"></i>
          Cargando...
        </div>
      </catalogo>

      <br>
      <br>
      <br>
      <img src="{{asset('img/slogan3-dark.svg')}}" class="image is-hidden-touch" style="height: 100px; margin: 0 auto;">
      <img src="{{asset('img/slogan4-dark.svg')}}" class="image is-hidden-desktop" style="width: 250px; margin: 0 auto;">
      <br>
      <br>
    </section>
  </div>
{% endblock %}