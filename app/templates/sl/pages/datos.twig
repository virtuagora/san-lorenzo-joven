            <th>Presupuesto Asignado</th>
            <td>$600.000</td>
            <td>$750.000</td>
            <td>$1.350.000</td>
            <td>$1.750.000</td>
            <td>$1.935.000</td>
            <td>$2.223.000</td>
            <td>$3.001.050</td>
          </tr>
          <tr>
            <th>Propuestas Presentadas</th>
            <td>78</td>
            <td>109</td>
            <td>111</td>
            <td>64</td>
            <td>83</td>
            <td>92</td>
            <td>96</td>
