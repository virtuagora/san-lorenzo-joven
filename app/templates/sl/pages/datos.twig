            {label:'Norte',data:[5,7,7,2,5,10,10], borderColor: '#7768ae',
        backgroundColor: 'rgba(157, 42, 84, 0)', pointRadius:6, pointHoverRadius:8},
        {label:'Centro',data:[6,7,10,3,11,9,9], borderColor: '#3bb273',
        backgroundColor: 'rgba(157, 42, 84, 0)', pointRadius:6, pointHoverRadius:8},
        {label:'Sur',data:[7,4,4,5,10,8,8], borderColor: '#e1bc29',
        backgroundColor: 'rgba(157, 42, 84, 0)', pointRadius:6, pointHoverRadius:8}]" :height="300"></stat-lines>
        </div>
      </div>
      <div class="columns">
        <div class="column is-6">
          <h1 class="title is-3 has-text-centered has-text-primary">Género en las votaciones</h1>
          <div class="subtitle is-5 has-text-centered">(Años 2013,2016,2018 y 2019)</div>
          <stat-bars :the-labels="[2013,2016,2018,2019]" :the-data="[{label:'Masculinos',data:[256,162,546,414],borderWidth:2,borderColor: '#48639c',
        backgroundColor: 'rgba(72, 99, 157, 0.25)'},
        {label:'Femeninos',data:[519,315,1079,834],borderWidth:2,borderColor: '#f7996e',
        backgroundColor: 'rgba(247, 153, 110, 0.25)'}]" :height="300"></stat-bars>
        </div>
        <div class="column is-6">
          <h1 class="title is-3 has-text-centered has-text-primary">Edad de los votantes a lo largo de los años</h1>
          <div class="subtitle is-5 has-text-centered">(Años 2013,2016,2018 y 2019)</div>
          <stat-stacked-bars :the-labels="[2013,2016,2018,2019]" :the-data="[
      {label:'16 a 25',data:[140,91,252,202],borderWidth:2,borderColor: '#2274a5',
        backgroundColor: 'rgba(72, 99, 157, 0.25)'},
        {label:'26 a 35',data:[209,100,416,294],borderWidth:2,borderColor: '#f1c40f',
        backgroundColor: 'rgba(241, 196, 14, 0.25)'},
        {label:'36 a 45',data:[124,119,437,322],borderWidth:2,borderColor: '#f75c03',
        backgroundColor: 'rgba(247, 92, 2, 0.25)'},
